<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vingel.in.propertyanimation" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FlipAnimation"
            android:label="@string/title_activity_flip_animation"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".FlipProgrammatically"
            android:label="@string/title_activity_flip_progmmactically"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="vingel.in.propertyanimation.MainActivity" />
        </activity>
        <activity
            android:name=".ObjectorSet"
            android:label="@string/title_activity_objector_set"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="vingel.in.propertyanimation.MainActivity" />
        </activity>
        <activity
            android:name=".TadComplexAnimation"
            android:label="@string/title_activity_tab_complex_animation"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="vingel.in.propertyanimation.MainActivity" />
        </activity>
    </application>

</manifest>
